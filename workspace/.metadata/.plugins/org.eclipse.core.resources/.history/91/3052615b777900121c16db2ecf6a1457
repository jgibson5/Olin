 */
package net.joegibson.huffman;

/**
 * @author jgibson
 *
 */
public class HistogramTest {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		
		Histogram h = new Histogram();
		System.out.println(h);
		
		h.count("moose");
		System.out.println(h);
		
		h.count('a');
		h.count('o');
		System.out.println(h);
		
		System.out.println(h.freq('o'));

	}
}
