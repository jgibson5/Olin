public class AnimalTest {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		System.out.println("TEST");
		
		Dog d = new Dog();
		Cat c = new Cat();
		
		d.makeNoise();
		c.makeNoise();
		
		d.setSize(20);
		c.setSize(1);
		System.out.println(d.isBigger(c));

	}

}
